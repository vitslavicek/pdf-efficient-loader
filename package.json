{
  "name": "pdf-efficient-loader",
  "version": "1.0.0",
  "description": "Ultra memory-efficient Node.js library for PDF analysis with intelligent document type detection (scan/vector/text). Extracts text, counts images and vectors with <100MB RAM usage even for large PDFs with hundreds of images.",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "start": "node example.js"
  },
  "keywords": [
    "pdf",
    "pdf-parser",
    "pdf-extractor",
    "text-extraction",
    "image-count",
    "vector-detection",
    "memory-efficient",
    "low-memory",
    "document-classification",
    "scan-detection",
    "ocr-routing",
    "pdfjs",
    "pdf-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.82",
    "pdfjs-dist": "^5.4.394"
  }
}
